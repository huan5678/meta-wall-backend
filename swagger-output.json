                }
              },
              "required": [
                "name",
                "email",
                "password",
                "confirmPassword"
              ]
            }
          }
        ],
        "responses": {
          "200": {
            "description": "為新使用者產生 token",
            "schema": {
              "type": "object",
              "properties": {
                "status": {
                  "type": "boolean",
                  "example": true
                },
                "user": {
                  "type": "object",

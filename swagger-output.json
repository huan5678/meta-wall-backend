    "/users/sign_up": {
      "post": {
        "tags": [
          "Users - 使用者"
        ],
        "description": "使用者註冊頁面 API",
        "parameters": [
          {
            "name": "body",
            "in": "body",
            "type": "object",
            "required": true,
            "description": "資料格式",
            "schema": {
              "type": "object",
              "properties": {
                "name": {
                  "type": "string",
                  "example": "Malenia5"
                },
                "email": {
                  "type": "string",
                  "example": "Malenia5@eldenring.io"
                },
                "password": {
